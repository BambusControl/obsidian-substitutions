  },
  "license": "Apache-2.0",
  "dependencies": {
  },
  "devDependencies": {
    "@codemirror/language": "^6.9.3",
    "@codemirror/state": "^6.3.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^18.11.18",
    "@types/papaparse": "^5.3.7",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.20.2",
    "esbuild-sass-plugin": "^3.1.0",
    "jest": "^29.5.0",
    "obsidian": "latest",
    "ts-jest": "^29.1.1",
    "tslib": "^2.4.1",
