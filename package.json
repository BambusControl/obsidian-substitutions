{
  "name": "obsidian-substitutions",
  "version": "0.1.2-NEXT",
  "description": "Obsidian plugin for substituting fragments of text.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "export-schema": "typescript-json-schema --required --strictNullChecks --validationKeywords --ignoreErrors --refs --id https://raw.githubusercontent.com/BambusControl/obsidian-substitutions/0.1.2-NEXT/resources/save-data-schema.json --out ./resources/save-data-schema.json tsconfig.json SaveData",
    "test": "jest"
  },
  "keywords": [
    "obsidian"
  ],
  "private": true,
  "repository": "https://github.com/BambusControl/obsidian-substitutions",
  "author": {
    "name": "BambusControl",
